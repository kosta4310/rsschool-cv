{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-plusplus": "off",
    "eol-last": "off",
    "max-len": 0,
    "prefer-destructuring": "off",
    "indent": "off",
    "no-param-reassign": "off"
  }
}
