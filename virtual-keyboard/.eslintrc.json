{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    // "semi": "off",
    "no-plusplus": "off",
    "eol-last": "off",
    // 'no-tabs': 'off',
    // "no-alert": "off",
    "prefer-destructuring": "off",
    "indent": "off",
    "no-param-reassign": "off"
  }
}
